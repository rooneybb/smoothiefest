{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": ["airbnb-base", "prettier", "eslint:recommended", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "plugins": ["react"],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "class-methods-use-this": "off"
    }
}
